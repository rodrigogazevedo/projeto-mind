<%- include('./partials/headDashboard') %>
  <body>
    <section class="grid dashboard">
      <div class="item logo">Mind Education</div>
      <div class="item nav">
        <div class="search">
          <img id="btnSearch" alt="Buscar" src="../images/search.png"/>
          <input type="text" id="search-text" placeholder="Search..."/>
        </div>
        <ul class="acess-login">
          <% if (typeof user !== "undefined") { %>
            <li class="nav-item">
              <a class="nav-link" href="">Olá, <%= user.name %></a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#">Sair</a>
            </li>
            <% } else { %>
            <li class="nav-item">
              <a class="nav-link" href="#">Logout</a>
            </li>
            <% } %>
        </ul>
      </div>
      <div class="item content">
        content
      </div>
      <div class="item footer">
        <h5 class="item footer">Copyrights ©</h5>
        <h5 class="item footer footer-colored"> Mind Education</h5>
      </div>
    </section>
  </body>
